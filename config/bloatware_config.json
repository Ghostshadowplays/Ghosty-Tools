    {
      "id": "onedrive_uninstall",
      "name": "OneDrive",
      "description": "Uninstall OneDrive and remove sync folders",
      "category": "OneDrive",
      "safety_level": "moderate",
      "requires_admin": true,
      "requires_restart": false,
      "windows_versions": ["10", "11"],
      "check_command": "Get-Process -Name 'OneDrive' -ErrorAction SilentlyContinue",
      "commands": [
        "taskkill /f /im OneDrive.exe",
        "Start-Sleep -Seconds 2",
        "if (Test-Path \"$env:SystemRoot\\System32\\OneDriveSetup.exe\") { Start-Process -FilePath \"$env:SystemRoot\\System32\\OneDriveSetup.exe\" -ArgumentList '/uninstall' -Wait }",
        "if (Test-Path \"$env:SystemRoot\\SysWOW64\\OneDriveSetup.exe\") { Start-Process -FilePath \"$env:SystemRoot\\SysWOW64\\OneDriveSetup.exe\" -ArgumentList '/uninstall' -Wait }",
        "Remove-Item -Path \"$env:UserProfile\\OneDrive\" -Force -Recurse -ErrorAction SilentlyContinue",
        "Remove-Item -Path \"$env:LocalAppData\\Microsoft\\OneDrive\" -Force -Recurse -ErrorAction SilentlyContinue",
        "Remove-Item -Path \"$env:ProgramData\\Microsoft OneDrive\" -Force -Recurse -ErrorAction SilentlyContinue"
      ]
    },
    {
      "id": "disable_telemetry_registry",
      "name": "Disable Telemetry",
      "description": "Disable Windows telemetry and diagnostic data collection via registry",
      "category": "Telemetry And Privacy",
      "safety_level": "moderate",
      "requires_admin": true,
      "requires_restart": true,
      "windows_versions": ["10", "11"],
      "check_command": "Get-ItemProperty -Path 'HKLM:\\SOFTWARE\\Policies\\Microsoft\\Windows\\DataCollection' -Name 'AllowTelemetry' -ErrorAction SilentlyContinue",
      "commands": [
        "New-Item -Path 'HKLM:\\SOFTWARE\\Policies\\Microsoft\\Windows\\DataCollection' -Force -ErrorAction SilentlyContinue",
        "Set-ItemProperty -Path 'HKLM:\\SOFTWARE\\Policies\\Microsoft\\Windows\\DataCollection' -Name 'AllowTelemetry' -Value 0 -Type DWord -Force"
      ]
    },
    {
      "id": "disable_activity_history",
      "name": "Disable Activity History",
      "description": "Disable Windows activity history and timeline",
      "category": "Telemetry And Privacy",
      "safety_level": "safe",
      "requires_admin": true,
      "requires_restart": false,
      "windows_versions": ["10", "11"],
      "check_command": "Get-ItemProperty -Path 'HKLM:\\SOFTWARE\\Policies\\Microsoft\\Windows\\System' -Name 'EnableActivityFeed' -ErrorAction SilentlyContinue",
      "commands": [
        "New-Item -Path 'HKLM:\\SOFTWARE\\Policies\\Microsoft\\Windows\\System' -Force -ErrorAction SilentlyContinue",
        "Set-ItemProperty -Path 'HKLM:\\SOFTWARE\\Policies\\Microsoft\\Windows\\System' -Name 'EnableActivityFeed' -Value 0 -Type DWord -Force",
        "Set-ItemProperty -Path 'HKLM:\\SOFTWARE\\Policies\\Microsoft\\Windows\\System' -Name 'PublishUserActivities' -Value 0 -Type DWord -Force",
        "Set-ItemProperty -Path 'HKLM:\\SOFTWARE\\Policies\\Microsoft\\Windows\\System' -Name 'UploadUserActivities' -Value 0 -Type DWord -Force"
      ]
    },
